state:
    record: Map<string, float>
    latency: Map<string, float>

init():
    pass

req(rpc):
	set(record, get(rpc, 'rpcid'), current_time())
	send(rpc, Down)

resp(rpc):
    rpcid = get(rpc, 'rpcid')
    match get(record, rpcid):
        Some(t) =>
            lat = time_diff(current_time(), t)
            set(latency, rpcid, lat)
        None =>
            pass
    send(rpc, Up)
