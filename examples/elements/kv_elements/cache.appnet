state:
	cache: Map<string, string>
    record_cache: Map<uint, string>

init():
    set(cache, 'bomb', 'bomb')

req(rpc):
    res = get(cache, get(rpc, 'key'))
	match res:
		Some(name) =>
            match name:
                'cached' =>
                    send(err('cache'), Up)
                'bomb' =>
                    send(err('bomb'), Up)
		None =>
            pass
    rpc_id = get(rpc, 'rpc_id', 'uint32')
    set(record_cache, rpc_id, get(rpc, 'key'))
	send(rpc, Down)

resp(rpc):
    body = get(record_cache, get(rpc, 'rpc_id', 'uint32'))
    match body:
        Some(name) => 
            set(cache, name, 'cached')
        None =>
            pass
	send(rpc, Up)
