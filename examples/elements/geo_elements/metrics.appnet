state
{
	record: Map<uint, Instant>
  latency: Vec<float>
}

fn init() {
}

fn req(rpc_req) {
	record.set(rpc_id(), current_time());
	send(rpc_req, NET);
}

fn resp(rpc_resp) {
  match(record.get(rpc_id())) {
    Some(t) => {
      lat := time_diff(current_time(), t);
      latency.set(latency.size(), lat);
    }
    None => {
    }
  };
  send(rpc_resp, APP);
}
