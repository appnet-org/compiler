state
{
	@consistency(strong) @combiner(LWW) @persistence(true)
	acl: Map<string, string>
}

fn init() {
	acl.set('test', 'No');
}

fn req(rpc_req) {
	match (acl.get(rpc_req.get('user'))) {
		Some(permission) => {
			match (permission) {
				'Yes' => {
					send(rpc_req, NET);
				}
				'No' => {
					send(err('acl'), APP);
				}
				_ => {
				}
			};
		}
		None => {
			send(rpc_req, NET);
		}
	};
}

fn resp(rpc_resp) {
    send(rpc_resp, APP);
}
