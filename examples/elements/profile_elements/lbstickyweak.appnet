state {
	@consistency(weak) @combiner(sum) @persistence(true)
    lb_tab: Map<string, string>
}

fn init() {

}

fn req(rpc_req) {
    match (lb_tab.get(rpc_req.get('locale'))) {
		Some(dst) => {
            // do nothing
		}
		None => {
            lb_tab.set(rpc_req.get('locale'), '1');
		}
	};

	send(rpc_req, NET);
}

fn resp(rpc_resp) {
	send(rpc_resp, APP);
}
